{% extends 'base.html.twig' %}

{% block title %}{{ parent()}} - Registro{% endblock %}

{% block body %}
<div class="container">
    <h1 class="h3 mb-3 font-weight-normal text-center">Registro de usuario</h1>

    {% for message in app.flashes('correcto') %}
        <div class="alert alert-success">
            {{ message }}
        </div>
    {% endfor %}

    {{ form(formulario)}}
</div>
{% endblock %}
